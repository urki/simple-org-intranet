{% extends '::white.museum.base.html.twig' %}
    
               
{% block title %}{{ title }}{% endblock title %}


{% block contentheader %}
{{ parent() }}

      <div class="masthead">
        <ul class="nav nav-pills pull-right">
          <li class="active"><a href="{{ path('main_menu') }}">{{ 'Home'|trans }}</a></li>
          {% if is_granted('ROLE_USER') %}
           <li><a href="{{ path('logout') }}">{{ 'Logout'|trans }}</a></li>
           {% else %}
            <li><a href="{{ path('login') }}">{{ 'Login'|trans }}</a></li>
           {% endif %}
        </ul>
        <h3 class="muted"> <a href="{{ path('main_menu') }}">
                    <img  data-toggle="tooltip" title="{{ "Home"|trans }}" src="{{ asset('bundles/menu/img/home64.png') }}" class="shadow img-circle shadow">

            </a>{{ title }} 
                    {% if subtitle is defined  %}
                        {{ subtitle }}
                    {% endif %}
        </h3>
      </div>

      <hr>

      
{% endblock contentheader %}
{% block content %}{% endblock content %}
{% block contentfoot %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
    <script type="text/javascript" src="{{ asset('bundles/survey/js/chosen.jquery.js') }}"></script>
     <script type="text/javascript"> $(".chzn-select").chosen(); $(".chzn-select-deselect").chosen({allow_single_deselect:true}); </script>
   {# @TODO how to modify to use just one jquery - the last one? no 1.6.4 #}
    <script type="text/javascript" src="{{ asset('bundles/survey/js/jquery.js') }}"></script>
    
    <script type="text/javascript" src="{{ asset('bundles/survey/js/bootstrap-collapse.js') }}"></script>   
    
{% endblock contentfoot %} 
    
   

