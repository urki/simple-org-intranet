{% extends 'SurveyBundle::layout2.html.twig' %}

{% set title = 'naslov6a' %}


{% block content %}
  {{ parent() }}

  <img id="flag" src="images/flags/pk.png"/>
  
  
  
<div class="container">
    <h1>Survey</h1>
    <p>
        
     
<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Time</th>
            <th>Temperature</th>
            <th>Moisture</th>
            <th>Note</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('survey_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{% if entity.time %}{{ entity.time|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.temperature }}</td>
            <td>{{ entity.moisture }}</td>
            <td>{{ entity.note }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('survey_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('survey_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('survey_new') }}">
            Create a new entry
        </a>
    </li>
</ul>

    

    </p>
</div>
{% endblock %}




